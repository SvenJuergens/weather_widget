{
    "name": "svenjuergens/weather-widget",
    "type": "typo3-cms-extension",
    "description": "TYPO3 Weather Widget with the help of Wttr.in",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Sven Juergens",
			"role": "Developer"
		}
	],
    "require": {
        "typo3/cms-core": "^12.4 || ^13.0"
    },
    "autoload": {
        "psr-4": {
            "SvenJuergens\\WeatherWidget\\": "Classes"
        }
    },
	"support": {
		"issues": "https://github.com/SvenJuergens/weather_widget/issues"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "weather_widget"
		}
	}
}
